func (c *WsClient) parseJSONData[T any](body []byte) *T {
	var raw struct {
		Data T `json:"data"`
	}
	if err := json.Unmarshal(body, &raw); err != nil {
		return nil
	}
	return &raw.Data
}

func (c *WsClient) parseGift(body []byte) *GiftData {
	return c.parseJSONData[GiftData](body)
}

func (c *WsClient) parseSuperChat(body []byte) *SuperChatMsgData {
	return c.parseJSONData[SuperChatMsgData](body)
}

func (c *WsClient) parseGiftStarProcess(body []byte) *GiftStarProcessData {
	return c.parseJSONData[GiftStarProcessData](body)
}

func (c *WsClient) parseOnlineRankCount(body []byte) *OnlineRankCountData {
	return c.parseJSONData[OnlineRankCountData](body)
}